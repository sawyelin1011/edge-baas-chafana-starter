    "generate": "edge-baas generate config/*.yaml"
  },
  "dependencies": {
    "chanfana": "^2.6.3",
    "hono": "^4.6.20",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.13.0",
    "@types/service-worker-mock": "^2.0.4",
    "typescript": "^5.6.0",
    "wrangler": "^4.55.0"
  }
}